<VBox alignment="CENTER" prefHeight="455.0" prefWidth="773.0" spacing="20.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <ImageView fitHeight="191.0" fitWidth="505.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@imagenes/cc6d2f46dcd424900b0458126d45d042.jpg"/>
    </ImageView>
    <HBox alignment="CENTER" prefHeight="62.0" prefWidth="733.0" spacing="20.0">
        <VBox.margin>
            <Insets/>
        </VBox.margin>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Cantidad de jugadores"/>
        <ComboBox fx:id="cantidadJugadores" prefHeight="25.0" prefWidth="57.0"
                  style="-fx-background-color: lightgray;"/>
    </HBox>
    <Button fx:id="iniciarPartida" onAction="#onHelloButtonClick" prefHeight="30.0" prefWidth="149.0" text="Jugar" />
</VBox>
